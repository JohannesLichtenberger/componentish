{
  "name": "transfomer",
  "version": "1.0.0",
  "description": "A simple application transformer",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tscript": "tsc --init --declaration --allowSyntheticDefaultImports --target esnext --outDir lib",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "del-cli ./lib/* && npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "transform": "node app.js --name=\"Basic Application\" --input=\"./samples/sample_1/application.json\"",
    "start": "npm run build && npm run transform",
    "check-dependencies": "npx madge --circular --extensions ts ./"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/fs-extra": "^5.0.4",
    "@types/node": "^10.11.6",
    "@types/yargs": "^12.0.1",
    "del-cli": "^1.1.0",
    "fs-extra": "^7.0.0",
    "typescript": "^3.1.1",
    "yargs": "^12.0.2"
  }
}
