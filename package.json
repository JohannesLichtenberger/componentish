{
    "name": "transfomer",
    "version": "1.0.0",
    "description": "A simple application transformer",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "tscript": "tsc --init --declaration --allowSyntheticDefaultImports --target esnext --outDir lib",
        "type-check": "tsc --noEmit",
        "type-check:watch": "npm run type-check -- --watch",
        "build": "del-cli ./lib/* && npm run build:types && npm run build:js",
        "build:types": "tsc --emitDeclarationOnly",
        "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
        "transform": "node app.js --input=\"./samples/sample_1/application.json\" --output=\"apps\" --name=\"MyApp\"",
        "start": "npm run build && npm run transform",
        "check-dependencies": "npx madge --circular --extensions ts ./",
        "format:fix": "pretty-quick --staged",
        "precommit": "run-s format:fix lint",
        "lint": "tslint 'src/**/*.{ts, tsx}'",
        "prettier": "prettier --config ./.prettierrc --write \"src/**/*{.ts,.js,.json,.css,.scss}\""
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/cli": "^7.0.0",
        "@babel/core": "^7.0.0",
        "@babel/plugin-proposal-async-generator-functions": "^7.1.0",
        "@babel/plugin-proposal-class-properties": "^7.1.0",
        "@babel/plugin-proposal-decorators": "^7.1.2",
        "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
        "@babel/plugin-transform-regenerator": "^7.0.0",
        "@babel/polyfill": "^7.0.0",
        "@babel/preset-env": "^7.1.0",
        "@babel/preset-typescript": "^7.1.0",
        "@types/fs-extra": "^5.0.4",
        "@types/node": "^10.11.6",
        "@types/yargs": "^12.0.1",
        "del-cli": "^1.1.0",
        "eslint-plugin-tslint": "^3.0.0",
        "fs-extra": "^7.0.0",
        "husky": "^1.1.2",
        "npm-run-all": "^4.1.3",
        "prettier": "^1.14.3",
        "pretty-quick": "^1.7.0",
        "tslint": "^5.11.0",
        "tslint-config-prettier": "^1.15.0",
        "tslint-config-standard": "^8.0.1",
        "typescript": "^3.1.1",
        "typescript-eslint-parser": "^20.0.0",
        "yargs": "^12.0.2"
    },
    "dependencies": {}
}
