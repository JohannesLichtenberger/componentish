{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended", "tslint-config-standard", "tslint-config-prettier"],
    "jsRules": {},
    "rules": {
        "ordered-imports": true,
        "no-console": {
            "severity": "warning",
            "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
        },
        "interface-name": [true, "never-prefix"]
    }
}
